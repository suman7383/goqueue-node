{
  "name": "@suman2001/goqueue-client",
  "version": "1.0.0",
  "description": "Node.js client SDK for GoQueue server (supports JSON and Protobuf)",
  "license": "ISC",
  "author": "sumanmukherjee2001@gmail.com",
  "type": "commonjs",
  "main": "dist/client.js",
  "types": "dist/client.d.ts",
  "exports": {
    ".": {
      "import": "./dist/client.js",
      "require": "./dist/client.js",
      "types": "./dist/client.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && node scripts/copyProtos.js",
    "gen:pbjs": "pbjs -t static-module -w commonjs -o src/proto/message_pb.js src/proto/message.proto",
    "gen:pbts": "pbts -o src/proto/message_pb.d.ts src/proto/message_pb.js",
    "gen:proto": "npm run gen:pbjs && npm run gen:pbts"
  },
  "dependencies": {
    "axios": "^1.13.1",
    "protobufjs": "^7.5.4"
  },
  "devDependencies": {
    "protobufjs-cli": "^1.1.3"
  }
}
